{
  "name": "wallet-server",
  "version": "0.4.150",
  "main": "lib/index.js",
  "config": {
    "dockerport": 4444
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "run-s format:fix fix lint tsc && ef-tspm && cp ./src/schemas/*.gql ./lib/schemas  && cp ./src/templates/handlebars/*.hbs ./lib/templates/handlebars/ && cp ./src/assets/*.jpg ./lib/assets/",
    "start": "npm run build && node ./lib/index.js",
    "dev": "run-s watch",
    "watch": "npx nodemon --watch 'src/**/*' -e ts,graphql,gql --exec ts-node --files -r tsconfig-paths/register ./src/index.ts",
    "test": "jest --coverage",
    "docker:build": "docker build -t $npm_package_name:$npm_package_version --target dev .",
    "docker:build_prod": "docker build --target prod -t $npm_package_name:$npm_package_version .",
    "docker:run": "docker run -p 127.0.0.1:$npm_package_config_dockerport:4444 --name $npm_package_name -d $npm_package_name:$npm_package_version",
    "docker:dev": "docker rm --force $npm_package_name ; run-s docker:build docker:run",
    "fix": "node node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json --fix --force",
    "gen-jwt": "npx ts-node --files -r tsconfig-paths/register ./scripts/gen-jwt.ts",
    "tsc": "node node_modules/typescript/bin/tsc",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json}\"",
    "format:all": "prettier --config ./.prettierrc --write \"src/**/*{.ts,.js,.json}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@blockbrothers/firebasebb": "^0.11.4",
    "@blockbrothers/legacy-simple-crypto": "^1.0.0",
    "@blockbrothers/nodemailer-sendgrid-transport": "^0.3.0",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/microservices": "^8.0.6",
    "@sendgrid/client": "^7.4.2",
    "@sendgrid/mail": "^7.4.2",
    "@sentry/node": "^5.20.0",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v3-core": "^1.0.0",
    "@uniswap/v3-sdk": "^3.5.1",
    "apollo-datasource": "^0.3.1",
    "apollo-datasource-rest": "^0.5.0",
    "apollo-server-caching": "^0.4.0",
    "apollo-server-express": "^2.15.0",
    "auto-bind": "^1.2.1",
    "aws-sdk": "^2.610.0",
    "axios": "^0.21.1",
    "bclient": "^0.1.7",
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "bluebird": "^3.7.2",
    "bson": "^4.5.3",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^2.11.0",
    "dotenv": "^6.2.0",
    "eth-url-parser": "^1.0.3",
    "ethers": "^5.3.1",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.3",
    "graphql-type-json": "0.3.2",
    "handlebars": "^4.7.6",
    "https": "^1.0.0",
    "json2csv": "^4.5.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "merge-graphql-schemas": "^1.5.8",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.9",
    "mongoose-long": "^0.3.2",
    "node-cron": "^2.0.3",
    "node-rsa": "1.1.1",
    "nodemailer": "^6.3.1",
    "pem-jwk": "2.0.0",
    "qrcode": "^1.4.4",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "simple-crypto-js": "^2.5.0",
    "simple-uniswap-sdk": "^3.4.2",
    "speakeasy": "^2.0.0",
    "subscriptions-transport-ws": "^0.10.0",
    "uuid": "^3.4.0",
    "winston": "^3.1.0",
    "winston-mongodb": "^5.0.7"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/auto-bind": "^1.2.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.162",
    "@types/node-cron": "^2.0.2",
    "@types/nodemailer": "^6.2.2",
    "@types/qrcode": "^1.3.3",
    "@types/speakeasy": "^2.0.4",
    "@types/uuid": "^3.4.4",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "lodash.chunk": "^4.2.0",
    "mongodb-memory-server": "^7.4.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.8.0",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.0.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.10.1",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "private": true,
  "engines": {
    "node": "16"
  }
}
